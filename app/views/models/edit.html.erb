<% title t(:edit_object, :object => t(:model, :scope => [ :marketplace ])) %>

<% form_for :model, :url => object_path, :html => { :method => :put, :multipart => true } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :model => @model } %>
  
  <%= f.submit_with_cancel(:submit => t(:save)) %>
<% end %>
