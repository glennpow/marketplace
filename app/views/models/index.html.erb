<% if @make %>
  <% title t(:models_for_object, :scope => [ :marketplace, :models ], :count => @indexer.collection.total_entries, :object => h(@make.name)) %>
<% else %>
  <% title tp(:model, :scope => [ :marketplace ]) %>
<% end %>

<%= render_indexer(@indexer) %>

<% if is_editor_of?(@make) %>
  <% page_action icon_link_to(:add, t(:create_object, :object => t(:make, :scope => [ :marketplace ])), new_manufacturer_make_path(@manufacturer)) %>
<% end %>
