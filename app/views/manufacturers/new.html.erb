<% title t(:create_object, :object => t(:manufacturer, :scope => [ :marketplace ])) %>

<% form_for :manufacturer, :url => objects_path, :html => { :multipart => true } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :manufacturer => @manufacturer } %>

  <%= f.submit_with_cancel(:submit => t(:create)) %>
<% end %>
