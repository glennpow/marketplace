<% if @vendor %>
  <% title t(:inventory_for_object, :scope => [ :marketplace ], :object => @vendor.name) %>
<% elsif @product %>
  <% title t(:prices_for_object, :scope => [ :marketplace ], :count => @indexer.collection.total_entries, :object => @product.full_name) %>
<% end %>

<%= render_indexer(@indexer) %>

<% if is_editor_of?(@vendor) %>
  <% page_action icon_link_to(:add_large, t(:add_object, :object => tp(:product, :scope => [ :marketplace ])), set_current_organization_path(@vendor.organization_id, :redirect => products_path)) %>
<% end %>
