<% title t(:create_object, :object => t(:product, :scope => [ :marketplace ])) %>

<%= render_heading(t(:create_object, :object => t(:product, :scope => [ :marketplace ]))) %>

<% form_for :product, :url => objects_path, :html => { :multipart => true } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :product => @product } %>

  <%= f.submit t(:create) %>
<% end %>
