<%= render_row(
  indexer,
  make,
  [
    link_to_resource(make),
    link_to_resource(make.manufacturer),
    h(make.available_models.length),
    h(make.available_products.length),
  ],
  :actions => is_editor_of?(make) ? [
    icon_link_to(:edit, t(:edit), edit_object_path(make)),
    icon_link_to(:delete, t(:delete), make, :confirm => t(:are_you_sure), :method => :delete)
  ] : nil) %>
