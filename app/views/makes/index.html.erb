<% if @manufacturer %>
  <% title t(:makes_for_object, :scope => [ :marketplace, :makes ], :count => @indexer.collection.total_entries, :object => h(@manufacturer.name)) %>
<% else %>
  <% title tp(:make, :scope => [ :marketplace ]) %>
<% end %>

<%= render_indexer(@indexer) %>

<% if is_editor_of?(@manufacturer) %>
  <% page_actions << icon_link_to(:add, t(:create_object, :object => t(:make, :scope => [ :marketplace ])), new_manufacturer_make_path(@manufacturer)) %>
<% end %>
