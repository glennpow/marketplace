<% title t(:create_object, :object => t(:make, :scope => [ :marketplace ])) %>

<%= render_heading(t(:create_object, :object => t(:make, :scope => [ :marketplace ]))) %>

<% form_for :make, :url => objects_path, :html => { :multipart => true } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :make => @make } %>
  
  <%= f.submit t(:create) %>
<% end %>

