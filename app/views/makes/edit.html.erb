<% title t(:edit_object, :object => t(:make, :scope => [ :marketplace ])) %>

<%= render_heading(t(:edit_object, :object => t(:make, :scope => [ :marketplace ]))) %>

<% form_for :make, :url => object_path, :html => { :method => :put, :multipart => true } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :make => @make } %>
  
  <%= f.submit t(:save) %>
<% end %>
